{
    "num_classes": 2,
    "model_name": "INATNet",
    "batch_size": 4,
    "num_epochs": 50,
    "learning_rate": 0.001,
    "weight_decay": 0.0001,
    "optimizer": "adam",
    "image_size": 360,
    "num_workers": 4,
    "pin_memory": true,
    "use_augmentation": true,
    "random_flip": true,
    "random_crop": true,
    "rotation_degrees": 15,
    "use_scheduler": false,
    "scheduler_type": "reduce_on_plateau",
    "scheduler_patience": 5,
    "scheduler_factor": 0.5,
    "scheduler_min_lr": 1e-07,
    "early_stopping": false,
    "early_stopping_patience": 10,
    "early_stopping_min_delta": 0.001,
    "save_best_only": false,
    "save_frequency": 5,
    "checkpoint_dir": "checkpoints",
    "custom_progress_callback": true,
    "log_dir": "logs",
    "tensorboard": false,
    "log_frequency": 10,
    "device": "cpu",
    "mixed_precision": true
}